<p id="notice"><%= notice %></p>

<h1>Listing Shop Stuffs</h1>

<table>
  <thead>
    <tr>
      <th>Shop</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @shop_stuffs.each do |shop_stuff| %>
      <tr>
        <td><%= shop_stuff.shop %></td>
        <td><%= link_to 'Show', shop_stuff %></td>
        <td><%= link_to 'Edit', edit_shop_stuff_path(shop_stuff) %></td>
        <td><%= link_to 'Destroy', shop_stuff, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Shop stuff', new_shop_stuff_path %>
