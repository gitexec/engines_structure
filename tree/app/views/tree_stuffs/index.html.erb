<p id="notice"><%= notice %></p>

<h1>Listing Tree Stuffs</h1>

<table>
  <thead>
    <tr>
      <th>Tree</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tree_stuffs.each do |tree_stuff| %>
      <tr>
        <td><%= tree_stuff.tree %></td>
        <td><%= link_to 'Show', tree_stuff %></td>
        <td><%= link_to 'Edit', edit_tree_stuff_path(tree_stuff) %></td>
        <td><%= link_to 'Destroy', tree_stuff, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tree stuff', new_tree_stuff_path %>
