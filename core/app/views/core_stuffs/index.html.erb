<p id="notice"><%= notice %></p>

<h1>Listing Core Stuffs</h1>

<table>
  <thead>
    <tr>
      <th>Core</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @core_stuffs.each do |core_stuff| %>
      <tr>
        <td><%= core_stuff.core %></td>
        <td><%= link_to 'Show', core_stuff %></td>
        <td><%= link_to 'Edit', edit_core_stuff_path(core_stuff) %></td>
        <td><%= link_to 'Destroy', core_stuff, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Core stuff', new_core_stuff_path %>
